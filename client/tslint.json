{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "quotemark": [
            true,
            "single",
            "jsx-double"
        ],
        "semicolon": [
            false
        ],
        "no-var-requires": false,
        "member-access": false,
        "arrow-parens": false,
        "trailing-comma": false,
        "max-classes-per-file": false,
        "curly": false,
        "max-line-length": [
            true,
            140
        ],
        "object-literal-sort-keys": false,
        "radix": false
    },
    "rulesDirectory": []
}