{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "quotemark": [
            true,
            "single",
            "jsx-double"
        ],
        "semicolon": [
            false
        ],
        "no-var-requires": false,
        "member-access": false,
        "arrow-parens": false
    },
    "rulesDirectory": []
}