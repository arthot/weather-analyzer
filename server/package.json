{
  "name": "weather-analyzer",
  "version": "2.0.0",
  "private": true,
  "scripts": {
    "build": "tsc",
    "start": "NODE_ENV=development nodemon --watch 'src/**/*.ts' --ignore 'src/**/*.spec.ts' --exec 'ts-node' -r tsconfig-paths/register src/server.ts",
    "test": "NODE_ENV=test mocha --opts mocha.opts",
    "test:all": "NODE_ENV=test LOG_LEVEL=silent nyc mocha --opts mocha.opts --recursive 'src/**/*.spec.ts'",
    "migrate": "NODE_ENV=development DEBUG=knex:query knex migrate:latest  --knexfile ./knexfile.ts",
    "migrate:test": "NODE_ENV=test DEBUG=knex:query knex migrate:latest  --knexfile ./knexfile.ts",
    "lint": "tslint src",
    "coverage": "nyc report"
  },
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/chai-http": "^3.0.5",
    "@types/cheerio": "^0.22.10",
    "@types/express": "^4.16.1",
    "@types/knex": "^0.15.2",
    "@types/lodash": "^4.14.121",
    "@types/loglevel": "^1.5.4",
    "@types/mocha": "^5.2.6",
    "@types/p-queue": "^3.0.0",
    "@types/proxyquire": "^1.3.28",
    "@types/request-promise-native": "^1.0.15",
    "chai": "^4.2.0",
    "chai-http": "^4.2.1",
    "mocha": "^6.0.0",
    "nodemon": "^1.18.10",
    "nyc": "^13.3.0",
    "proxyquire": "^2.1.0",
    "source-map-support": "^0.5.10",
    "ts-node": "^8.0.2",
    "tsconfig-paths": "^3.8.0",
    "typescript": "^3.3.3"
  },
  "dependencies": {
    "cheerio": "^1.0.0-rc.2",
    "date-fns": "^1.30.1",
    "express": "^4.16.4",
    "express-validator": "^5.3.1",
    "knex": "^0.16.3",
    "lodash": "^4.17.11",
    "loglevel": "^1.6.1",
    "loglevel-plugin-prefix": "^0.8.4",
    "nocache": "^2.0.0",
    "objection": "^1.6.0",
    "p-queue": "^3.0.0",
    "request": "^2.88.0",
    "request-promise-native": "^1.0.7",
    "sqlite3": "^4.0.6"
  }
}
