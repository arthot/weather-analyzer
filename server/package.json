{
  "name": "weather-analyzer",
  "version": "2.0.0",
  "private": true,
  "scripts": {
    "build": "tsc",
    "start": "cross-env NODE_ENV=development nodemon --watch 'src/**/*.ts' --ignore 'src/**/*.spec.ts' --exec 'ts-node' -r tsconfig-paths/register src/server.ts",
    "test": "cross-env NODE_ENV=test mocha --opts mocha.opts",
    "test:all": "cross-env NODE_ENV=test LOG_LEVEL=silent nyc mocha --opts mocha.opts --recursive src/**/*.spec.ts",
    "migrate": "cross-env NODE_ENV=development DEBUG=knex:query knex migrate:latest",
    "migrate:test": "cross-env NODE_ENV=test DEBUG=knex:query knex migrate:latest",
    "lint": "tslint src",
    "coverage": "nyc report"
  },
  "devDependencies": {
    "@types/chai": "^4.0.10",
    "@types/chai-http": "^3.0.3",
    "@types/cheerio": "^0.22.6",
    "@types/date-fns": "^2.6.0",
    "@types/ejs": "^2.5.0",
    "@types/express": "^4.11.0",
    "@types/knex": "0.0.68",
    "@types/lodash": "^4.14.91",
    "@types/loglevel": "^1.5.3",
    "@types/mocha": "^2.2.45",
    "@types/p-queue": "^1.1.0",
    "@types/pg": "^7.4.1",
    "@types/proxyquire": "^1.3.28",
    "@types/request-promise-native": "^1.0.10",
    "chai": "^4.1.2",
    "chai-http": "^3.0.0",
    "cross-env": "^5.1.3",
    "mocha": "^4.0.1",
    "nodemon": "^1.14.3",
    "nyc": "^11.4.1",
    "proxyquire": "^1.8.0",
    "source-map-support": "^0.5.0",
    "ts-node": "^4.1.0",
    "tsconfig-paths": "^2.6.0",
    "typescript": "^2.6.2"
  },
  "dependencies": {
    "cheerio": "^1.0.0-rc.2",
    "date-fns": "^1.29.0",
    "ejs": "^2.5.7",
    "express": "^4.16.2",
    "express-validator": "^4.3.0",
    "knex": "^0.14.2",
    "lodash": "^4.17.4",
    "loglevel": "^1.6.0",
    "loglevel-plugin-prefix": "^0.5.3",
    "mustache-express": "^1.2.5",
    "nocache": "^2.0.0",
    "objection": "^0.9.4",
    "p-queue": "^2.3.0",
    "reflect-metadata": "^0.1.10",
    "request": "^2.83.0",
    "request-promise-native": "^1.0.5",
    "sqlite3": "^3.1.13"
  }
}
